{
  "family": "final-frontend-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "final-frontend",
      "image": "${FRONTEND_ECR_URI}:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}