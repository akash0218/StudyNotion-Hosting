{
  "family": "final-backend-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "final-backend",
      "image": "${BACKEND_ECR_URI}:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5050,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}